extends template.pug

block scripts
  link(rel='stylesheet', href='./bootstrap-5.0.1-dist/css/bootstrap.min.css')
  link(rel='stylesheet', href='./js/gojs/assets/css/style.css')
  link(rel='stylesheet', href='./css/style.css')
  script(src='https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/js/bootstrap.bundle.min.js' integrity='sha384-gtEjrD/SeCtmISkJkNUaaKMoLD0//ElJ19smozuHV6z3Iehds+3Ulb9Bn9Plx0x4' crossorigin='anonymous')
  script(src='./js/gojs/release/go.js')
  script(src='./js/gojs/extensions/Figures.js')
  script(src='./js/gojs/extensions/DataInspector.js')
  script(src='./js/init.js')
  script(src='./js/export.js')
  script(src='./js/export2.js')
  script(src='./js/download.js')
  script(src='./js/todayTime.js')
  script(src='./js/uploadAndImportFromExcel.js', defer='')
  script(src='./js/reuse2.js', defer='')
  script(src='./js/classes/Node.js')
  script(src='./js/classes/Link.js')
  script(src='./js/classes/Preset.js')
block content
  #mySidebar.leftMenu
    form.formstogether(action='/', method='get', id='homeForm')
      a(href="javascript:{}" onclick="document.getElementById('homeForm').submit();") Home
    form(action='/upload', method='post', id='uploadDBForm')
      input#diagramId(type='hidden', name='diagramId' value=diagramId)
      input#uploadData(type='hidden', name='uploadData')
      input#uploadLinks(type='hidden', name='uploadLinks')
      a(href="javascript:{}" onclick="document.getElementById('uploadDBForm').submit();") Save
      .dropdown
        a(href="javascript:{}") Export
        .dropdown-content
          a(href="javascript:{}", onclick='save2()', class="dropdown-item") Export Personal Data
          a#savedModel(href="javascript:{}", onclick='save()', class="dropdown-item") Export DataObj
    a(href="javascript:{}" data-bs-toggle="modal" data-bs-target="#formModal") import
    a(href="javascript:{}" data-bs-toggle="modal" data-bs-target="#dataObjModal")  reuse DataObj
    a(href="'https://github.com/ersanuensal/Advanced-Programming/blob/main/README.md") Documentation
  .contentPages
    input#downloadData(type='hidden', name='downloadData' value=downloadData)
    input#downloadLinks(type='hidden', name='downloadLinks' value=downloadLinks)
    .centercontainer
      #myPaletteDiv.palette
      #myOverviewDiv.overview
      #myDiagramDiv.diagram
      #myInspectorDiv.inspector
      div(class="modal fade" id="dataObjModal" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1"
      aria-labelledby="ModalLabel" aria-hidden="true")
        div(class="modal-dialog")
          div(class="modal-content")
            div(class="modal-header")
              h5(class="modal-title" id="ModalLabel">Import from Excel)
              button(type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close")
            div(class="modal-body")
              <div class="row mb-3">
                div
                  button#resave(onclick='asd(1)')  Save Preset 
                  button#reload(onclick='asdf(1)')  Load Preset 
                  button#reedit(onclick='')  Edit Preset 
                div
                #saveDataObj.form-popup
                  form#asd.form-container(action='')
                    h1 Save a Preset
                    label#dataObjNameLabel(for='dataObjName')
                      b Name
                    input#dataObjName(type='text' placeholder='Enter Name' required='')
                    label(for='email')
                      b Description
                    input#dataObjDes(type='text' placeholder='Enter Description' required='')
                    label(for='email')
                      b Color
                    input#dataObjColor(type='color' value='#000000' required='')
                    label(for='email')
                      b PersonalData
                    input#dataObjPer(type='checkbox')
                    button.btn(type='button' onclick='resave()' class="btn btn-primary") Save Preset
                    button.btn.cancel(type='button' onclick='asd(0)') Close
                  form#asdf.form-container(action='')
                    select#selectNumber
                      option(value='A') Select a preset:
                    button.btn(type='button' onclick='reload()') Load Preset
                    button.btn.cancel(type='button' onclick='asdf(0)') Close
                    button.btn.btn-secondary(type='button' data-bs-dismiss='modal') Cancel
      div(class="modal fade" id="formModal" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1"
      aria-labelledby="ModalLabel" aria-hidden="true")
        div(class="modal-dialog")
          div(class="modal-content")
            div(class="modal-header")
              h5(class="modal-title" id="ModalLabel">Import from Excel)
              button(type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close")
            div(class="modal-body")
              <div class="row mb-3">
                form#uploadForm
                  div
                    .row.mb-3
                      input(type='file' class = 'form-control' id = 'inpFile' name='inpFile' required accept='.xlsx, .xls, .xlsm')
                    .row.mb-3
                      label(class = 'form-label' for='sheetsName') un check if first row is not a header.
                      input(id = 'headerRow' class="form-check-input" type="checkbox" checked)
                  div
                    button.btn.btn-secondary(type='button' data-bs-dismiss='modal') Cancel
                    button.btn.btn-primary(type='submit') upload

    script(src="./bootstrap-5.0.1-dist/js/bootstrap.min.js")     

